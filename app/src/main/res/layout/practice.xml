<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/lin"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:orientation="vertical"
    tools:context=".CompleteDoctorProfile" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="300sp"
        android:layout_weight="0.10" >

        <TableLayout
            android:layout_width="481sp"
            android:layout_height="match_parent" >

            <TableRow
                android:id="@+id/tableRow5"
                android:layout_width="wrap_content"
                android:layout_height="0sp"
                android:layout_weight="1" >
            </TableRow>

            <TableRow
                android:id="@+id/tableRow3"
                android:layout_width="wrap_content"
                android:layout_height="0sp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/dateList"
                    style="@style/button_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center|left"
                    android:text="Drug Prod. Name:"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/drugProNameET"
                    android:layout_width="0sp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:imeOptions="flagNoFullscreen"
                    android:hint="Drug Product Name"
                    android:inputType="text|textCapCharacters|textCapWords"
                    android:textSize="25sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow4"
                android:layout_width="wrap_content"
                android:layout_height="0sp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/availMemoryTV"
                    style="@style/button_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center|left"
                    android:text="Drug Prod. Lot. no.:"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/drugProLotET"
                    android:layout_width="0sp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"

                    android:imeOptions="flagNoFullscreen"
                    android:ems="10"
                    android:hint="Drug Product Lot. No."
                    android:inputType="text|textCapCharacters|textCapWords|textCapSentences"
                    android:textSize="25sp" />
            </TableRow>

            <TableRow
                android:id="@+id/wettingFluidTR"
                android:layout_width="wrap_content"
                android:layout_height="0sp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/dateOFYearLB"
                    style="@style/button_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center|left"
                    android:text="Filter Cat. No. :"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/filterCatET"
                    android:layout_width="0sp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:hint="Filter Cat. No."
                    android:imeOptions="flagNoFullscreen"
                    android:inputType="text|textCapCharacters|textCapWords"
                    android:textSize="25sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow7"
                android:layout_width="wrap_content"
                android:layout_height="0sp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/testTypeList"
                    style="@style/button_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center|left"
                    android:text="Filter Lot. No. :"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/filterLotET"
                    android:layout_width="0sp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:hint="Filter Lot. No."
                    android:inputType="text|textCapCharacters|textCapWords"
                    android:imeOptions="flagNoFullscreen"
                    android:textSize="25sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow8"
                android:layout_width="wrap_content"
                android:layout_height="0sp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/totalMemoryTV"
                    style="@style/button_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center|left"
                    android:text="Wetting Fluid :"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/wettingFluidET"
                    android:layout_width="0sp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ems="10"
                    android:hint="Wetting Fluid"
                    android:imeOptions="flagNoFullscreen"
                    android:inputType="text|textCapCharacters|textCapWords"
                    android:textSize="25sp" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow9"
                android:layout_width="wrap_content"
                android:layout_height="0sp"
                android:layout_weight="1" >

                <TextView
                    android:id="@+id/ramTV"
                    style="@style/button_text"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center|left"
                    android:text="Description*:"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp" />

                <EditText
                    android:id="@+id/descriptionET"
                    android:layout_width="257sp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:hint="(Optional)"
                    android:imeOptions="flagNoFullscreen"
                    android:inputType="text|textCapCharacters|textCapWords"
                    android:textSize="25sp" />
            </TableRow>
        </TableLayout>

        <LinearLayout
            android:id="@+id/eee"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.01"
            android:orientation="vertical" >

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="57sp"
                android:layout_marginLeft="10sp"
                android:layout_weight="0.08" >

                <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginTop="10sp"
                    android:layout_weight="1" >

                    <TextView
                        android:id="@+id/textView8"
                        style="@style/button_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center|left"
                        android:text="Bubble Point :"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="25sp" />

                    <EditText
                        android:id="@+id/bubblepointET"
                        android:layout_width="100sp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:hint="Bubble Point"
                        android:imeOptions="flagNoFullscreen"
                        android:inputType="number|numberDecimal"
                        android:textSize="25sp" />

                    <Spinner
                        android:id="@+id/bubblePointSP"
                        style="@style/button_text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_width="wrap_content"
                    android:layout_height="0sp"
                    android:layout_weight="1" >

                    <TextView
                        android:id="@+id/textView9"
                        style="@style/button_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center|left"
                        android:text="Process Area:"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="25sp" />

                    <EditText
                        android:id="@+id/prductionAreaET"
                        android:layout_width="0sp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:hint=" Area"
                        android:imeOptions="flagNoFullscreen"
                        android:inputType="text|textCapCharacters|textCapWords|textCapSentences"
                        android:textSize="25sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/diffRow1"
                    android:layout_width="wrap_content"
                    android:layout_height="0sp"
                    android:layout_weight="1" >

                    <TextView
                        android:id="@+id/diffDetail1"
                        style="@style/button_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center|left"
                        android:text="Pore Size :"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="25sp" />

                    <EditText
                        android:id="@+id/poreSizeET1"
                        android:layout_width="0sp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:imeOptions="flagNoFullscreen"
                        android:hint="Pore Size"
                        android:inputType="number|numberDecimal"
                        android:textSize="25sp" />

                    <TextView
                        android:id="@+id/waterDetail1"
                        style="@style/button_text"
                        android:layout_width="120sp"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="µm"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="25sp" />
                </TableRow>

                <TableRow
                    android:id="@+id/bpRow1"
                    android:layout_width="wrap_content"
                    android:layout_height="0sp"
                    android:layout_weight="1" >

                    <TextView
                        android:id="@+id/diffDetail2"
                        style="@style/button_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center|left"
                        android:text="Filter Size :"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="25sp" />

                    <EditText
                        android:id="@+id/filterSizeET1"
                        android:layout_width="0sp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:hint="Filter Size"
                        android:imeOptions="flagNoFullscreen"
                        android:inputType="number|numberDecimal"
                        android:textSize="25sp" />

                    <Spinner
                        android:id="@+id/filterSizeSP1"
                        style="@style/button_text"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow6"
                    android:layout_width="wrap_content"
                    android:layout_height="0sp"
                    android:layout_weight="1" >

                    <TextView
                        android:id="@+id/TextView01"
                        style="@style/button_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="Test Gas :"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="25sp" />

                    <EditText
                        android:id="@+id/testAirET"
                        android:layout_width="0sp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:ems="10"
                        android:inputType="text"
                        android:imeOptions="flagNoExtractUi|actionDone"
                        android:text="Compressed Air"
                        android:textSize="25sp" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10sp"
        android:layout_marginTop="10sp"
        android:layout_weight="0.01" >

        <Button
            android:id="@+id/backBT"
            style="@style/button_text"
            android:layout_width="250sp"
            android:layout_height="70sp"
            android:layout_marginLeft="10sp"
            android:layout_weight="1"
            android:background="@drawable/re_button"
            android:text="Back" />

        <Button
            android:id="@+id/scanBarcodeBT"
            style="@style/button_text"
            android:layout_width="250sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@drawable/re_button"
            android:text="Barcode" />

        <Button
            android:id="@+id/templateBT"
            style="@style/button_text"
            android:layout_width="250sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@drawable/re_button"
            android:text="Template" />

        <Button
            android:id="@+id/resetBT"
            style="@style/button_text"
            android:layout_width="250sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@drawable/re_button"
            android:text="Reset" />

        <Button
            android:id="@+id/startTestBT"
            style="@style/button_text"
            android:layout_width="250sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@drawable/re_button"
            android:text="Start Test" />
    </LinearLayout>

</LinearLayout>